{
  "name": "my-dapp",
  "version": "0.1.0",
  "description": "TypeScript DApp",
  "private": true,
  "type": "module",
  "dependencies": {
    "@calindra/cartesify-backend": "^0.1.2",
    "express": "^4.18.2",
    "openapi-fetch": "^0.7",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "viem": "^1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20",
